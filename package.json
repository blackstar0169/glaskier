{
  "name": "glaskier",
  "version": "2.0.0",
  "description": "Random jaskier glitch sounds",
  "main": "main.js",
  "scripts": {
    "lint": "npx eslint",
    "dev": "tsx watch src/index.ts",
    "deploy": "npm run build && node --enable-source-maps dist/deploy-commands.js",
    "start": "npm run dev",
    "start:prod": "npm run build && node --enable-source-maps dist/index.js",
    "build": "tsup src/index.ts src/deploy-commands.ts --minify",
    "pm2:start": "pm2 start process.json",
    "pm2:stop": "pm2 stop process.json",
    "pm2:delete": "pm2 delete process.json"
  },
  "author": "GrosBalek",
  "license": "ISC",
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "discord.js": "^14.19.3",
    "dotenv": "^17.2.3",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.15",
    "moment": "^2.30.1",
    "mp3-duration": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@stylistic/eslint-plugin": "^4.4.1",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "volta": {
    "node": "20.19.5"
  }
}
